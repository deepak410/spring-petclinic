pipeline{
    agent any
        stages{
            stage('git clone'){
                steps{
                git url: 'https://github.com/deepak410/simple-java-maven-app.git'
            }
            }
            stage('build using maven'){
                steps{
                sh "mvn package"
                }
            }
        
            stage('Download') {
            steps {
                sh 'echo "artifact file" > generatedFile.txt'
            }
            }
        }
        
    }
